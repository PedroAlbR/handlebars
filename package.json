{
  "name": "nymag-handlebars",
  "version": "1.3.0",
  "description": "A collection of helpers and partials for handlebars",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "lint": "eslint helpers partials test index.js",
    "test-local": "npm run lint && istanbul cover _mocha",
    "test": "npm run lint && istanbul cover _mocha --report lcovonly -- -R spec && cat ./coverage/lcov.info | coveralls && rm -rf ./coverage",
    "generate-readme": "node docs/generate-readme.js && git add README.md"
  },
  "pre-commit": [
    "generate-readme"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nymag/nymag-handlebars.git"
  },
  "keywords": [
    "handlebars"
  ],
  "author": "New York Media",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nymag/nymag-handlebars/issues"
  },
  "homepage": "https://github.com/nymag/nymag-handlebars#readme",
  "devDependencies": {
    "chai": "^3.5.0",
    "chalk": "^1.1.3",
    "coveralls": "^2.11.15",
    "documentation": "^4.0.0-beta.18",
    "eslint": "^3.13.0",
    "istanbul": "^0.4.5",
    "mocha": "^3.2.0",
    "pre-commit": "^1.2.2"
  },
  "dependencies": {
    "comma-it": "0.0.7",
    "glob": "^7.1.1",
    "handlebars": "^4.0.6",
    "handlebars-helpers": "^0.7.5",
    "he": "^1.1.0",
    "helper-yaml": "^0.1.0",
    "lodash": "^4.17.4",
    "moment": "^2.17.1",
    "randomstring": "^1.1.5",
    "speakingurl": "^11.0.0",
    "striptags": "^2.1.1"
  }
}
